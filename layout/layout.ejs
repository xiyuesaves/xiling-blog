<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title><%- config.title %></title>
	<!-- 主要样式 -->
	<%- css('css/main.css') %>
	<%- css('css/iconfont.css') %>

	<!-- 脚本 -->
	<%- js('js/main.js')%>
</head>
<body>
	<div id="main" style="background-image: url(<%- theme.backgroundImage %>);">
		<!-- 顶部导航 -->
		<header id="header-nav">
			<%- include('header-nav.ejs') %>
		</header>

		<!-- 左侧栏 -->
		<aside id="left-aside">
			<%- include('left-aside.ejs') %>
		</aside>

		<!-- 正文 -->
		<div id="main-block">
			<%- body %>
		</div>

		
	</div>
</body>
</html>